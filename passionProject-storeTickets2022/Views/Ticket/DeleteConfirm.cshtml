@model passionProject_storeTickets2022.Models.TicketDto
@{
    ViewBag.Title = "DeleteConfirm";
}

<h2>Are you sure you want to delete this Ticket?</h2>
<a href="/Ticket/List">Cancel</a>
<h3>Name: @Model.EventName @Model.EventLocation</h3>
    <form action="/Ticket/Delete/@Model.TicketId" method="POST">
        <input type="submit" value="Confirm Delete" />
    </form>
