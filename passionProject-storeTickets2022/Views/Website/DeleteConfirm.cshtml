@model passionProject_storeTickets2022.Models.Website
@{
    ViewBag.Title = "DeleteConfirm";
}

<h2>Are you sure you want to delete this Website?</h2>
<a href="/Website/List">Cancel</a>
<h3>Name: @Model.WebsiteName @Model.WebsiteType/h3>
<form action="/Website/Delete/@Model.WebsiteId" method="POST">
    <input type="submit" value="Confirm Delete" />
</form>